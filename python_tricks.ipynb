   "id": "0b502366",
   "metadata": {},
   "outputs": [],
   "source": [
    "word = \"hello world\"\n",
    "result = list(word)\n",
    "left = 0\n",
    "right = len(result) - 1\n",
    "while left < right:\n",
    "    result[right], result[left] = result[left], result[right]\n",
    "    left += 1\n",
    "    right -= 1\n",
    "print(result)"
   ]
  }
